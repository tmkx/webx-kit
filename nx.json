{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "cli": {
    "packageManager": "pnpm"
  },
  "defaultBase": "main",
  "nxCloudId": "686fa76549686cbdade1394a",
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"]
    },
    "lint:type": {
      "dependsOn": ["^lint:type"],
      "outputs": []
    },
    "pretest": {
      "cache": true,
      "outputs": ["{projectRoot}/output"]
    },
    "test": {
      "dependsOn": ["build", "pretest", "^test"]
    }
  }
}
