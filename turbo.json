{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist"]
    },
    "lint:type": {
      "dependsOn": ["^lint:type"],
      "outputs": []
    },
    "pretest": {
      "outputs": ["output"]
    },
    "test": {
      "dependsOn": ["build", "pretest", "^test"]
    }
  }
}
