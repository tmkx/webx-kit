{
  "name": "@webx-kit/rsbuild-plugin",
  "version": "0.1.4",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./manifest": {
      "types": "./src/manifest.ts",
      "default": "./src/manifest.ts"
    },
    "./tailwind": {
      "types": "./src/tailwind.ts",
      "default": "./src/tailwind.ts"
    }
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./manifest": {
        "types": "./dist/manifest.d.ts",
        "default": "./dist/manifest.js"
      },
      "./tailwind": {
        "types": "./dist/tailwind.d.ts",
        "default": "./dist/tailwind.js"
      }
    },
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chrome",
    "extension",
    "webext",
    "rsbuild"
  ],
  "repository": {
    "url": "https://github.com/tmkx/webx-kit.git",
    "directory": "packages/rsbuild-plugin"
  },
  "scripts": {
    "prepack": "tsdown",
    "lint:type": "tsc --noEmit"
  },
  "dependencies": {
    "@types/chrome": "^0.0.326",
    "@webx-kit/core-plugin": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.3.22",
    "@rspack/core": "^1.3.12",
    "tsdown": "^0.12.7",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^0.7.0"
  }
}
