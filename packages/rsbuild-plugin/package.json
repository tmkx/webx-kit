{
  "name": "@webx-kit/rsbuild-plugin",
  "version": "0.2.0",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./manifest": {
      "types": "./src/manifest.ts",
      "default": "./src/manifest.ts"
    },
    "./tailwind": {
      "types": "./src/tailwind.ts",
      "default": "./src/tailwind.ts"
    }
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./manifest": {
        "types": "./dist/manifest.d.ts",
        "default": "./dist/manifest.js"
      },
      "./tailwind": {
        "types": "./dist/tailwind.d.ts",
        "default": "./dist/tailwind.js"
      }
    },
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chrome",
    "extension",
    "webext",
    "rsbuild"
  ],
  "repository": {
    "url": "https://github.com/tmkx/webx-kit.git",
    "directory": "packages/rsbuild-plugin"
  },
  "scripts": {
    "prepack": "tsdown",
    "lint:type": "tsc --noEmit"
  },
  "dependencies": {
    "@types/chrome": "^0.1.1",
    "chokidar": "^4.0.3",
    "jiti": "^2.5.1",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.4.14",
    "tailwindcss": "^4.1.11",
    "tsdown": "^0.13.3",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.0.0",
    "tailwindcss": "^4.0.0"
  }
}
