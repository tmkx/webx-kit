{
  "name": "@webx-kit/modernjs-plugin",
  "version": "0.1.2",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./manifest": {
      "types": "./src/manifest.ts",
      "default": "./src/manifest.ts"
    },
    "./tailwind": {
      "types": "./src/tailwind.ts",
      "default": "./src/tailwind.ts"
    }
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./manifest": {
        "types": "./dist/manifest.d.ts",
        "default": "./dist/manifest.js"
      },
      "./tailwind": {
        "types": "./dist/tailwind.d.ts",
        "default": "./dist/tailwind.js"
      }
    },
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chrome",
    "extension",
    "webext",
    "webpack"
  ],
  "repository": {
    "url": "https://github.com/tmkx/webx-kit.git",
    "directory": "packages/modernjs-plugin"
  },
  "scripts": {
    "prepack": "tsup",
    "lint:type": "tsc --noEmit"
  },
  "dependencies": {
    "@types/chrome": "^0.0.306",
    "@webx-kit/core-plugin": "workspace:*"
  },
  "devDependencies": {
    "@modern-js/app-tools": "^2.65.1",
    "@modern-js/utils": "^2.65.1",
    "@rsbuild/core": "catalog:",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@modern-js/app-tools": "^2.40.0",
    "@modern-js/utils": "^2.40.0",
    "@rsbuild/core": "^1.0.0"
  }
}
